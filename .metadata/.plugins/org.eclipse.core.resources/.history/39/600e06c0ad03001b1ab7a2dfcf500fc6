package com.nttdata.basic;

public class MinVariables {
	public static void main(String[] args) {
		
		int intValue=Integer.MIN_VALUE;
		float floatValue=Float.MIN_VALUE;
		byte byteValue=Byte.MIN_VALUE;
		short shortValue=Short.MIN_VALUE;
		long longValue=Long.MIN_VALUE;
		double doubleValue=Double.MIN_VALUE;
		boolean booValue=Boolean.FALSE;
		
		char c='S';
		
		System.out.println("value of integer is "+ intValue);
		System.out.println("value of float is "+ floatValue);
		System.out.println("value of byte is "+ byteValue);
		System.out.println("value of short is "+ shortValue);
		System.out.println("value of long is "+ longValue);
		System.out.println("value of double is "+ doubleValue);
		System.out.println("value of boolean is "+ booValue);
	}

}
